;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	arya.stark.lan. root.stark.lan. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	arya.stark.lan.
arya	IN	A	192.168.10.8
ned    IN  A   192.168.10.10
router  IN  A   192.168.10.254
